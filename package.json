  "scripts": {
    "start:do-app": "ng serve @dongkap/do-app --port 4200",
    "start:do-mock": "ng serve @dongkap/do-mock --port 4201",
    "clean:manifest:github": "node node_modules/rimraf/bin ./dist/docs/manifest.webmanifest",
    "copy:manifest:github": "node node_modules/cpx2/bin \"./@dongkap/do-app/src/manifest/github/manifest.webmanifest\" \"./dist/docs\"",
    "build:do-core": "ng build @dongkap/do-core",
    "build:do-theme": "ng build @dongkap/do-theme",
    "build:do-shared": "ng build @dongkap/do-shared",
    "build:do-auth": "ng build @dongkap/do-auth",
    "build:do-extra": "ng build @dongkap/do-extra",
    "build:do-sys": "ng build @dongkap/do-sys",
    "build:do-app": "ng build @dongkap/do-app --prod",
    "build:do-app:github": "ng build @dongkap/do-app --configuration=github --output-path dist/docs --base-href=/satpol-ngx/ --deleteOutputPath=false && npm run clean:manifest:github && npm run copy:manifest:github",
    "copy:do-theme": "node node_modules/cpx2/bin \"./@dongkap/do-theme/src/styles/**/*\" \"./dist/@dongkap/do-theme/styles\"",
    "p:do-core": "npm run build:do-core && cd dist/@dongkap/do-core && npm pack",
    "p:do-theme": "npm run build:do-theme && npm run copy:do-theme && cd dist/@dongkap/do-theme && npm pack",
    "p:do-shared": "npm run build:do-shared && cd dist/@dongkap/do-shared && npm pack",
    "p:do-auth": "npm run build:do-auth && cd dist/@dongkap/do-auth && npm pack",
    "p:do-extra": "npm run build:do-extra && cd dist/@dongkap/do-extra && npm pack",
    "p:do-sys": "npm run build:do-sys && cd dist/@dongkap/do-sys && npm pack",
    "install:do-core": "npm run p:do-core && npm r @dongkap/do-core && npm i -O ./dist/@dongkap/do-core/dongkap-do-core-0.0.1.tgz",
    "install:do-theme": "npm run p:do-theme && npm r @dongkap/do-theme && npm i -O ./dist/@dongkap/do-theme/dongkap-do-theme-0.0.1.tgz",
    "install:do-shared": "npm run p:do-shared && npm r @dongkap/do-shared && npm i -O ./dist/@dongkap/do-shared/dongkap-do-shared-0.0.1.tgz",
    "install:do-auth": "npm run p:do-auth && npm r @dongkap/do-auth && npm i -O ./dist/@dongkap/do-auth/dongkap-do-auth-0.0.1.tgz",
    "install:do-extra": "npm run p:do-extra && npm r @dongkap/do-extra && npm i -O ./dist/@dongkap/do-extra/dongkap-do-extra-0.0.1.tgz",
    "install:do-sys": "npm run p:do-sys && npm r @dongkap/do-sys && npm i -O ./dist/@dongkap/do-sys/dongkap-do-sys-0.0.1.tgz",
    "ng": "ng",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "conventional-changelog": "conventional-changelog",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@akveo/ng2-completer": "^9.0.1",
